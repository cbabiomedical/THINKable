<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thinkableproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThinkableProject">

        <activity android:name=".Suggestions" />
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".EnterPhoneActivity" />
        <activity android:name=".PreferencesSecPage" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".LandingPage"
            android:label="@string/title_activity_landing_page" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>